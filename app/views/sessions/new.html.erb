<div class="container well well-md">
  <%= form_for :session do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %> <br />

    <%= f.label :password %>
    <%= f.password_field :password %> <br />

    <%= f.submit "Login" %>
  <% end %>

  <h4><%= link_to "Create a New Account", new_user_path %></h3>
</div>
